        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_shopping_cart"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Contact_us_form"></activity>
        <activity android:name=".Purchase_return_form" />
        <activity android:name=".Purchase_list_form" />
