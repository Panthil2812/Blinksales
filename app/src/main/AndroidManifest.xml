<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rku.blinksales">

    <application
        android:fullBackupContent="true"
        android:icon="@drawable/ic_shopping_cart"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_shopping_cart"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash_screen" android:screenOrientation="locked">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Login_page"  android:screenOrientation="locked"> </activity>

        <activity android:name=".Signup_page"  android:screenOrientation="locked"></activity>

        <activity android:name=".MainActivity" android:screenOrientation="locked"></activity>
    </application>

</manifest>